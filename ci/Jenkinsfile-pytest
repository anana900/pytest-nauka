	stage('raport') 
	{
		try
		{
			sh "pwd"
			sh "ls -al"
			dir(".") {
				sh "pwd"
				sh "ls -al"
    			publishHTML([
    				 allowMissing: false,
    		         alwaysLinkToLastBuild: true,
    		         keepAll: true,
    		         reportDir: '',
    		         reportFiles: 'wyniki.html',
    		         reportName: 'Pytest-Raport-Test√≥w'
    		         ])
			}
		}
		catch(err)
		{
			echo err
		}
	}
}